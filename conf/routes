# Routes
# This file defines all application routes (Higher priority routes first)
# ~~~~

# An example controller showing a sample home page
GET     /agendas/:date                              controllers.AgendasController.find(date: String, request: Request)
GET     /agendas/:dateStart/:dateEnd                controllers.AgendasController.findWeek(dateStart: String, dateEnd: String, request: Request)
GET     /agendas/balance/:dateStart/:dateEnd        controllers.AgendasController.getBalanceWeek(dateStart: String, dateEnd: String, request: Request)
