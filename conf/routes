# Routes
# This file defines all application routes (Higher priority routes first)
# ~~~~

# An example controller showing a sample home page
GET     /agendas/:dateStart/:dateEnd                controllers.AgendasController.getWeeklyAgenda(dateStart: String, dateEnd: String, request: Request)
GET     /agendas/balance/:dateStart/:dateEnd        controllers.AgendasController.getWeeklyBalance(dateStart: String, dateEnd: String, request: Request)
